<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="it.sang.abroile">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Abroile"
        android:usesCleartextTraffic="true"
        tools:targetApi="m">
        <activity
            android:name=".admin.LaporanAdmin"
            android:exported="false" />
        <activity android:name=".admin.LoginAdmin" />
        <activity android:name=".admin.MasterPenyakit" />
        <activity android:name=".admin.AdminMain" />
        <activity android:name=".HasilGejala" />
        <activity android:name=".PerhitunganDiagnosa" />
        <activity android:name=".DiagnoseActivity" />
        <activity android:name=".TentangActivity" />
        <activity android:name=".BantuanActivity" />
        <activity android:name=".SplashscreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".InfoActivity" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>